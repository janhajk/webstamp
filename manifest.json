{
  "manifest_version": 2,

  "name": "Webstamp Torpedo",
  "description": "This extensions speed up webstamp.post.ch process",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
  "permissions": [
   "activeTab"
   ]
}